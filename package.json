{
  "name": "react-typescript-starter",
  "version": "0.1.0",
  "description": "React Typescript starter, with material-ui, storybook, and playroom",
  "repository": {
    "type": "git",
    "url": "https://github.com/michael-m-judd/react-typescript-starter.git"
  },
  "engines": {
    "node": "^10.15.3"
  },
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.1.2",
    "@material-ui/styles": "^4.2.0",
    "@types/enzyme": "^3.10.3",
    "@types/material-ui": "^0.21.6",
    "@types/node": "^11.13.5",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-test-renderer": "^16.8.3",
    "@types/styled-components": "^4.1.16",
    "i18next": "^17.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "react-router-dom": "^5.0.1",
    "react-scripts": "2.1.8",
    "styled-components": "^4.3.2",
    "typescript": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:playroom": "playroom start",
    "start:storybook": "start-storybook -p 6006 -c .storybook",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "build:storybook": "build-storybook",
    "build:playroom": "playroom build",
    "create:component": "ft tsx-component ./src/components",
    "create:page": "ft tsx-page ./src/pages",
    "lint:css": "sh ./scripts/lint_css.sh",
    "lint:js": "sh ./scripts/lint_js.sh",
    "lint": "yarn lint:css; yarn lint:js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.0.10",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.0.10",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-storysource": "^5.0.10",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.0.10",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-info": "^4.1.1",
    "@types/storybook__addon-knobs": "^4.0.5",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.4",
    "coveralls": "^3.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "folder-template": "^0.1.4",
    "husky": "^1.3.1",
    "jest-styled-components": "^6.3.3",
    "playroom": "^0.9.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-test-renderer": "^16.8.6",
    "storybook-addon-jsx": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-loader": "^5.3.3"
  },
  "resolutions": {
    "webpack": "4.28.3"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/*.stories.tsx",
      "!**/types/**",
      "!**/test-helpers/**"
    ]
  }
}
